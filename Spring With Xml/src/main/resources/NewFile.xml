<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

   
   <bean id="eeeref" class="com.SpringwithXml.Interface.example.EEE">
     <property name="name" value="Jeya"/>
        <property name="id" value="023"/>
        <property name="dept" value="EEE" />
    </bean>
    
    <bean id="cseref" class="com.SpringwithXml.Interface.example.CSE">
     <property name="name" value="Jeya"/>
        <property name="id" value="023"/>
        <property name="dept" value="CSE" />
    </bean>
    
    <bean id="uni" class="com.SpringwithXml.Interface.example.University" >
    <property name="col" ref="cseref"/>    
    </bean>
    
</beans>